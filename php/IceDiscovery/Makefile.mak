top_srcdir	= ..

!include $(top_srcdir)\make\Make.rules.mak

SUBDIRS		= hello

$(EVERYTHING)::
	@for %i in ( $(SUBDIRS) ) do \
	    @echo "making $@ in %i" && \
	    cmd /c "cd %i && $(MAKE) -nologo -f Makefile.mak $@" || exit 1
